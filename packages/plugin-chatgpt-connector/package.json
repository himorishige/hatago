{
  "name": "@hatago/plugin-chatgpt-connector",
  "version": "0.1.0",
  "description": "ChatGPT MCP connector plugin with search and fetch tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./search": {
      "import": "./dist/search.js",
      "types": "./dist/search.d.ts"
    },
    "./fetch": {
      "import": "./dist/fetch.js",
      "types": "./dist/fetch.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hatago/core": "workspace:*",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "keywords": [
    "mcp",
    "chatgpt",
    "connector",
    "search",
    "fetch",
    "plugin"
  ],
  "license": "MIT"
}
